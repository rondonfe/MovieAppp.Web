@model Movie

<div class="film">
    <img src="/img/@Model.ImageUrl" alt="@Model.Title" />
    <p>@Model.Title</p>
    <p>@Model.Description</p>
    <p>@Model.Directors</p>
    <ul>
        @foreach (var player in Model.Players)
        {
            <li>@player</li>
        }
    </ul>
</div>
</div>